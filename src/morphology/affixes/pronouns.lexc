!! !!!Skolt Saami Pronoun Morphology 

LEXICON PRON_
!! The lexicon ''@LEXNAME@'' assigns the tag __+Pron__
+Pron: # ;

LEXICON PRON-PERS_
+Pron+Pers: # ;
LEXICON PRON-PERS-SG1_
+Pron+Pers+Sg1: # ;
LEXICON PRON-PERS-SG2_
+Pron+Pers+Sg2: # ;
LEXICON PRON-PERS-SG3_
+Pron+Pers+Sg3: # ;
LEXICON PRON-PERS-PL1_
+Pron+Pers+Pl1: # ;
LEXICON PRON-PERS-PL2_
+Pron+Pers+Pl2: # ;
LEXICON PRON-PERS-PL3_
+Pron+Pers+Pl3: # ;


LEXICON PRON-REL_
+Pron+Rel: # ;

!! !!!Pronouns


!! Pointing to all the pronominal subgroups

LEXICON Pronoun !!= * @CODE@
Personal ;      !!= * @CODE@
Demonstrative ; !!= * @CODE@
Reflexive ;     !!= * @CODE@
Interrogative ; !!= * @CODE@
! Reciprocal ;
 Relative ;	!!= * @CODE@ kååʹtt, mii
! Indefinite ;


!! !!Personal pronouns


!! Splitting according to person

LEXICON Personal           !!= * @CODE@
mon+Pron+Pers:0m perspron ;  !!= * @CODE@ The Epsilon  inserted 2015-02-11
ton+Pron+Pers:t perspron ;  !!= * @CODE@ The Epsilon  inserted 2015-02-11
!ton+Pron+Pers:0t perspron ;  !!= * @CODE@ The Epsilon  inserted 2015-02-11
son+Pron+Pers:0s perspron ;  !!= * @CODE@ The Epsilon  inserted 2015-02-11

LEXICON perspron
:u	    perspronoblsg ;
:uän    persprondu ;
:ån	    persprondu ;
:	    perspronpl ;
+Sg+Nom:on	K ;
+Du+Nom:ååna	K ;
+Pl+Loc:eeʹst	K ;

LEXICON perspronoblsg 
+Sg+Gen:u	      K ;
+Sg+Acc:u	      K ;
+Sg+Ill:ʹnne	      K ;
+Sg+Loc:ʹst	      K ;
+Sg+Com:in	      K ;
+Sg+Abe:utää	      K ;
+Ess:uʹnen	      K ;
+Par:uʹđed	      K ;



LEXICON persprondu
+Du+Nom:a		K ;
+Du+Gen:nai	      K ;
+Du+Acc:naid	      K ;
+Du+Ill:naid	      K ;
+Du+Loc:nast	      K ;
+Du+Com:nain	      K ;
+Du+Abe:naitää	      K ;
+Du+Ess:nan	      K ;

LEXICON perspronpl
+Pl+Nom:ij		K ; !!= * @CODE@ mij, tij, sij
+Pl+Gen:ij	      K ; !!= * @CODE@ mij, tij, sij
+Pl+Acc:iʹjjid	      K ; !!= * @CODE@ miʹjjid, tiʹjjid, siʹjjid
+Pl+Ill:iʹjjid	      K ; !!= * @CODE@ miʹjjid, tiʹjjid, siʹjjid
+Pl+Loc:eeʹst	      K ; !!= * @CODE@ meeʹst, teeʹst, seeʹst
+Pl+Com:ijvuiʹm     K ; !!= * @CODE@ mijvuiʹm, tijvuiʹm, sijvuiʹm
+Pl+Abe:ijtää	      K ; !!= * @CODE@ mijtää, tijtää, sijtää




!! !!Demonstrative pronouns


LEXICON Demonstrative !!= * @CODE@
tät+Pron+Dem:tä		SHORTDEM ;
tut+Pron+Dem:tu 	SHORTDEM ;
tõt+Pron+Dem:tõ 	SHORTDEM ;
tät+Pron+Dem:tää 	LONGDEM ;
tut+Pron+Dem:tuu 	LONGDEM ;
tõt+Pron+Dem:t  	TOTDEM ; !!= * @CODE@ The Epsilon has been removed 2015-05-13
tiet-aa:tie 		TIET-AA ;

LEXICON SHORTDEM !!= * @CODE@
+Sg+Nom:t	K ;
+Sg+Acc:n	K ;
+Sg+Gen:n	K ;
+Sg+Loc:ʹst	K ;
+Sg+Com:in	K ;
+Sg+Abe:ntää	K ;
+Pl+Nom:k	K ;
+Pl+Acc:id	K ;
