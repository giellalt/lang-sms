LEXICON GIGA
% miljard% : OVERMILLIONS ;

LEXICON HUNDREDSM

kueˊhtt:2    CUODIM ;
koumm:3  CUODIM ;
nellj:4 CUODIM ;

vitt:5  CUODIM ;
kutt:6  CUODIM ;
čiččâm:7  CUODIM ;


kääuˊc:8  CUODIM ;
ååuˊc:9   CUODIM ;

LEXICON CUODIM
čueˊđ: HUNDREDM ; !200, not 100

LEXICON HUNDREDM
      TENSM ;
      TEENSM ;
:%0   ONESM ;
:%0%0 MILJON ;

LEXICON TEENSM
:1 TEENM ;

LEXICON TEENM
õt:1     LOHKAIM ;
kuât:2     LOHKAIM ;
kon:3   LOHKAIM ;
nenj:4  LOHKAIM ;
vitt:5   LOHKAIM ;
kutt:6   LOHKAIM ;
čiččâm:7   LOHKAIM ;
käc:8   LOHKAIM ;
åc:9    LOHKAIM ;

LEXICON LOHKAIM
mlååi: MILJON ;

LEXICON TENSM
lååi:1%0 MILJON ;

kueˊhtt:2    LÅGEVM ;
koumm:3  LÅGEVM ;
nellj:4 LÅGEVM ;

vitt:5  LÅGEVM ;
kutt:6  LÅGEVM ;
čiččâm:7  LÅGEVM ;


kääuˊc:8  LÅGEVM ;
ååuˊc:9   LÅGEVM ;

LEXICON LÅGEVM
lååi:%0 MILJON ;
lo:     ONESM ;
lo%-õhtt% :   1MILJON ;

LEXICON ONEM
õhtt% : 1MILJON ;

LEXICON ONESM
kueˊhtt:2    MILJON ;
koumm:3  MILJON ;
nellj:4 MILJON ;
vitt:5  MILJON ;
kutt:6  MILJON ;
čiččâm:7  MILJON ;
kääuˊc:8  MILJON ;
ååuˊc:9   MILJON ; 

LEXICON 1MILJON
miljon% :1 OVERTHOUSANDS ;
miljon% :1  OVERTHOUSANDS ;

LEXICON MILJON
% miljon% : OVERTHOUSANDS ;

! =================
! Under the million
