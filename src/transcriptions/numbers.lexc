dohat: THOUSAND ;


LEXICON THOUSAND
UNDERTHOUSAND ;	   ! 1100-1999
:%0 THOUSANDTENS ;	   ! 1020-1099
:%0 TEENS ;	   ! 1010-1019
:%0%0 INITIALCARDINAL ;	   ! 1001-1009
